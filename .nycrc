{
  "include": [
    "src/**/*.vue"
  ],
  "exclude": [
    "node_modules/",
    "dist",
    "webpack",
    "**/**.spec.ts",
    "src/base/conf",
    "src/base/wp-plugins"
  ],
  "extension": [
    ".js"
  ],
  "require": [
    "ignore-styles",
    "babel-register"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html"
  ],
  "all": true,
  "sourceMap": false,
  "instrument": false
}